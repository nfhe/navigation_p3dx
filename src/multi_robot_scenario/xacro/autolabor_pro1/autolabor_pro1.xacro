<?xml version="1.0"?>
<!-- Revolute-Revolute Manipulator -->
<robot name="autolabor_pro1" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:property name="PI" value="3.1415926535897931"/>
  <xacro:property name="shaft_spacing_f" value="0.19649" />
  <xacro:property name="shaft_spacing_b" value="0.18988" />
  <xacro:property name="wheel_spacing_2" value="0.2643" />
  <xacro:property name="height" value="0.11913" />

  <xacro:include filename="$(find multi_robot_scenario)/xacro/autolabor_pro1/autolabor_pro1.gazebo" />

  <!-- Base Link -->
  <link name="base_link">
    <collision>
      <origin xyz="0 0 0.18288" rpy="0 0 0"/>
      <geometry>
        <box size="0.7248 0.4302 0.195"/>
      </geometry>
      <!--geometry>
        <mesh filename="package://multi_robot_scenario/meshes/autolabor_pro1/body_color.dae"/>
      </geometry-->
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://multi_robot_scenario/meshes/autolabor_pro1/body_color.dae"/>
      </geometry>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="39"/>
      <inertia ixx="0.7245" ixy="0.0" ixz="0.0" iyy="1.80" iyz="0.0" izz="2.2857"/>
    </inertial>
  </link>

  <link name="left_front_wheel">
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder length="0.0822" radius="0.127"/>
      </geometry>
      <!--geometry>
        <mesh filename="package://multi_robot_scenario/meshes/autolabor_pro1/wheel_color.dae"/>
      </geometry-->
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://multi_robot_scenario/meshes/autolabor_pro1/wheel_color.dae"/>
      </geometry>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.5"/>
      <inertia ixx="0.013" ixy="0.0" ixz="0.0" iyy="0.0074" iyz="0.0" izz="0.0074"/>
    </inertial>
  </link>

  <link name="left_back_wheel">
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder length="0.0822" radius="0.127"/>
      </geometry>
      <!--geometry>
        <mesh filename="package://multi_robot_scenario/meshes/autolabor_pro1/wheel_color.dae"/>
      </geometry-->
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://multi_robot_scenario/meshes/autolabor_pro1/wheel_color.dae"/>
      </geometry>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.5"/>
      <inertia ixx="0.013" ixy="0.0" ixz="0.0" iyy="0.0074" iyz="0.0" izz="0.0074"/>
    </inertial>
  </link>

  <link name="right_front_wheel">
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder length="0.0822" radius="0.127"/>
      </geometry>
      <!--geometry>
        <mesh filename="package://multi_robot_scenario/meshes/autolabor_pro1/wheel_color.dae"/>
      </geometry-->
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 ${PI}"/>
      <geometry>
        <mesh filename="package://multi_robot_scenario/meshes/autolabor_pro1/wheel_color.dae"/>
      </geometry>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 ${PI}"/>
      <mass value="1.5"/>
      <inertia ixx="0.013" ixy="0.0" ixz="0.0" iyy="0.0074" iyz="0.0" izz="0.0074"/>
    </inertial>
  </link>

  <link name="right_back_wheel">
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder length="0.0822" radius="0.127"/>
      </geometry>
      <!--geometry>
        <mesh filename="package://multi_robot_scenario/meshes/autolabor_pro1/wheel_color.dae"/>
      </geometry-->
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 ${PI}"/>
      <geometry>
        <mesh filename="package://multi_robot_scenario/meshes/autolabor_pro1/wheel_color.dae"/>
      </geometry>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 ${PI}"/>
      <mass value="1.5"/>
      <inertia ixx="0.013" ixy="0.0" ixz="0.0" iyy="0.0074" iyz="0.0" izz="0.0074"/>
    </inertial>
  </link>

  <joint name="joint_left_front" type="continuous">
    <parent link="base_link"/>
    <child link="left_front_wheel"/>
    <origin xyz="${shaft_spacing_f} ${wheel_spacing_2} ${height}" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.7"/>
  </joint>

  <joint name="joint_left_back" type="continuous">
    <parent link="base_link"/>
    <child link="left_back_wheel"/>
    <origin xyz="-${shaft_spacing_b} ${wheel_spacing_2} ${height}" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.7"/>
  </joint>

  <joint name="joint_right_front" type="continuous">
    <parent link="base_link"/>
    <child link="right_front_wheel"/>
    <origin xyz="${shaft_spacing_f} -${wheel_spacing_2} ${height}" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.7"/>
  </joint>

  <joint name="joint_right_back" type="continuous">
    <parent link="base_link"/>
    <child link="right_back_wheel"/>
    <origin xyz="-${shaft_spacing_b} -${wheel_spacing_2} ${height}" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.7"/>
  </joint>

    <!-- Add IMU Sensor -->
    <!-- <xacro:include filename="$(find multi_robot_scenario)/xacro/imu/imu.xacro" />
    <xacro:imu_sensor parent="base_link" name="imu" topic_ns="/imu">
      <origin xyz="0 0 0.285" rpy="0 0 0" />
    </xacro:imu_sensor> -->

    <!-- Add Velodyne VLP-16 LiDAR -->
    <xacro:include filename="$(find velodyne_description)/urdf/VLP-16.urdf.xacro"/>
    <xacro:VLP-16 parent="base_link" name="velodyne" topic="/velodyne_points" hz="10" samples="1800" gpu="false" min_angle="0.0" max_angle="6.28319" min_range="0.3" max_range="10.0">
      <origin xyz="0.125 0 0.35" rpy="0 0 0" />
    </xacro:VLP-16>

    <!-- Add D435i Camera -->
    <!-- <xacro:include filename="$(find realsense_gazebo_description)/xacro/_d435i.xacro.xacro"/>
    <xacro:sensor_d435i parent="base_link" name="d435i" topics_ns="d435i">
        <origin xyz="0.2 0 0.27" rpy="0 0 0" />
    </xacro:sensor_d435i> -->

    <!-- Add Camera -->
    <!-- <xacro:include filename="$(find multi_robot_scenario)/xacro/camera/autolabor_camera.xacro" />
    <xacro:cam parent="base_link" name="camera" xyz="0.25 0 0.28" rpy="0 0 0" /> -->

    <!-- Add Laser -->
    <xacro:include filename="$(find multi_robot_scenario)/xacro/laser/hokuyo.xacro" />
    <xacro:laser name="hokuyo" parent="base_link" xyz="0.125 0 0.31" rpy="0 0 0" meshes="package://multi_robot_scenario/meshes/laser" topic="/scan">
    </xacro:laser>
</robot>
